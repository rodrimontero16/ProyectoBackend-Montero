<div class="container">
    <h1 class="homeTitle">Bienvenidos ðŸ™Œ</h1>
    <div class="tableProds">
        <h3>Lista de productos:</h3>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Title</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Code</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                    <tr>
                        <th scope="row">{{id}}</th>
                        <td>{{title}}</td>
                        <td>{{description}}</td>
                        <td>${{price}}</td>
                        <td>{{code}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
