<div>
        <table class="table table-carts">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Titulo</th>
                    <th scope="col">Cantidad</th>
                    <th scope="col">Editar cantidad</th>
                    <th scope="col">Eliminar/Editar</th>
                </tr>
            </thead>
            <tbody id="tableProductsCarts">
                {{#if products}}  
                    {{#each products}}
                    {{#if @first}}
                        <h3>
                            ID Carrito: {{this.cartID}}
                        </h3>
                    {{/if}} 
                    <tr data-product-id="{{this._id}}">
                        <th scope="row">{{this._id}}</th>
                        <th>{{this.title}}</th>
                        <td>{{this.quantity}}</td>
                        <td class="edit-quantity">
                            <input type="number" name="quantity" value="{{this.quantity}}">
                            <button class="btn-carts">üñãÔ∏è</button>
                        </td>
                        <td>
                            <button class="btn-carts delete-prod-cart" id="{{this._id}}">‚ùå</button>
                        </td>
                    </tr>
                {{/each}}
            {{/if}}
            </tbody>
        </table>
</div>
